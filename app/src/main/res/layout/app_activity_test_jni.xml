<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".test.TestJniActivity">

    <TextView
        android:id="@+id/tvTestJni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvTestJniRust"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        app:layout_constraintTop_toBottomOf="@+id/tvTestJni" />

    <Button
        android:id="@+id/btnTestJni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="call jni"
        app:layout_constraintTop_toBottomOf="@+id/tvTestJniRust" />

</androidx.constraintlayout.widget.ConstraintLayout>